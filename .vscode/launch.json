{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "SchemaGeneratorUtility",
            "request": "launch",
            "mainClass": "us.dot.its.jpo.geojsonconverter.utils.SchemaGeneratorUtility",
            "projectName": "jpo-geojsonconverter",
            "args": ["--output", "${workspaceFolder}/jpo-geojsonconverter/src/main/resources/schemas"]
        },
        {
            "type": "java",
            "name": "GeoJsonConverter",
            "request": "launch",
            "mainClass": "us.dot.its.jpo.geojsonconverter.GeoJsonConverterApplication",
            "projectName": "jpo-geojsonconverter",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}